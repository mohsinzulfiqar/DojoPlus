{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport { useRouter } from 'next/router';\nimport styled from 'styled-components';\nimport { SuccessCheckout } from './components/SuccessCheckout';\nimport { CancelCheckout } from './components/CancelCheckout';\nconst Container = styled.div.withConfig({\n  displayName: \"CheckoutReturn__Container\",\n  componentId: \"sc-1fdq80z-0\"\n})([\"width:100%;height:100%;display:flex;align-items:center;justify-content:center;padding:0 1rem;\"]);\nconst Content = styled.div.withConfig({\n  displayName: \"CheckoutReturn__Content\",\n  componentId: \"sc-1fdq80z-1\"\n})([\"display:flex;flex-direction:column;justify-content:center;align-items:center;background-color:#fff;padding:1.5rem 3rem;border-radius:5px;min-width:350px;\"]);\nconst STATUS = {\n  cancel: 'cancel',\n  success: 'success'\n};\nexport const CheckoutReturn = () => {\n  const {\n    query\n  } = useRouter();\n\n  const onClick = () => {\n    if (window && query.returnTo) {\n      window.location.replace(query.returnTo);\n    }\n  };\n\n  if (!query.returnTo || !query.status || !STATUS[query.status]) {\n    return /*#__PURE__*/_jsx(Container, {\n      children: \"ups!! something is wrong :(\"\n    });\n  }\n\n  return /*#__PURE__*/_jsx(Container, {\n    children: /*#__PURE__*/_jsx(Content, {\n      children: query.status === 'success' ? /*#__PURE__*/_jsx(SuccessCheckout, {\n        onClick: onClick\n      }) : /*#__PURE__*/_jsx(CancelCheckout, {\n        onClick: onClick\n      })\n    })\n  });\n};","map":null,"metadata":{},"sourceType":"module"}