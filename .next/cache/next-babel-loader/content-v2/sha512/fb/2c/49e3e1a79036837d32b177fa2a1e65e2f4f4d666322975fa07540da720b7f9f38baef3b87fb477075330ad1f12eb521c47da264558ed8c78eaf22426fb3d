&& query.returnTo) {\n      window.location.replace(query.returnTo);\n    }\n  };\n\n  if (!query.returnTo || !query.status || !STATUS[query.status]) {\n    return /*#__PURE__*/_jsx(Container, {\n      children: \"ups!! something is wrong :(\"\n    });\n  }\n\n  return /*#__PURE__*/_jsx(Container, {\n    children: /*#__PURE__*/_jsx(Content, {\n      children: query.status === 'success' ? /*#__PURE__*/_jsx(StripeReturnSuccess, {\n        title: message.title,\n        message: message.content,\n        onClick: onClick\n      }) : /*#__PURE__*/_jsx(StripeReturnError, {\n        title: errorMessage.title,\n        message: errorMessage.content,\n        onClick: onClick\n      })\n    })\n  });\n};","map":null,"metadata":{},"sourceType":"module"}