{"ast":null,"code":"import { jsx as _jsx } from \"react/jsx-runtime\";\nimport React from 'react';\nimport { useRouter } from 'next/router';\nimport styled from 'styled-components';\nimport { StripeReturnSuccess } from './components/StripeReturnSuccess';\nimport { StripeReturnError } from './components/StripeReturnError';\nconst Container = styled.div.withConfig({\n  displayName: \"StripeReturn__Container\",\n  componentId: \"sc-1q7rri-0\"\n})([\"width:100%;height:100%;display:flex;align-items:center;justify-content:center;padding:0 1rem;\"]);\nconst Content = styled.div.withConfig({\n  displayName: \"StripeReturn__Content\",\n  componentId: \"sc-1q7rri-1\"\n})([\"display:flex;flex-direction:column;justify-content:center;align-items:center;background-color:#fff;padding:1.5rem 3rem;border-radius:5px;min-width:350px;\"]);\nconst STATUS = {\n  cancel: 'cancel',\n  success: 'success'\n};\nconst SUCCESS_MESSAGE = {\n  checkout: {\n    title: 'Payment Confirmed!!',\n    content: ' Your transaction has been completed successfully'\n  },\n  accountlink: {\n    title: 'Stripe Bank Account',\n    content: 'Stripe bank account updated successfully'\n  },\n  portal: {\n    title: 'Stripe Portal',\n    content: 'Session end Go back to app'\n  },\n  default: {\n    title: 'Return to app',\n    content: 'Go back to the app'\n  }\n};\nconst ERROR_MESSAGE = {\n  checkout: {\n    title: 'Payment Canceled!!',\n    content: ' Session checkout has been cancel'\n  },\n  default: {\n    title: 'Something is wrong',\n    content: 'Go back to the app'\n  }\n};\nexport const StripeReturn = () => {\n  const {\n    query\n  } = useRouter();\n  const message = typeof query.from === 'string' && SUCCESS_MESSAGE[query.from] ? SUCCESS_MESSAGE[query.from] : SUCCESS_MESSAGE.default;\n  const errorMessage = typeof query.from === 'string' && ERROR_MESSAGE[query.from] ? ERROR_MESSAGE[query.from] : ERROR_MESSAGE.default;\n\n  const onClick = () => {\n    if (window && query.returnTo) {\n      window.location.replace(query.returnTo);\n    }\n  };\n\n  if (!query.returnTo || !query.status || !STATUS[query.status]) {\n    return /*#__PURE__*/_jsx(Container, {\n      children: \"ups!! something is wrong :(\"\n    });\n  }\n\n  return /*#__PURE__*/_jsx(Container, {\n    children: /*#__PURE__*/_jsx(Content, {\n      children: query.status === 'success' ? /*#__PURE__*/_jsx(StripeReturnSuccess, {\n        title: message.title,\n        message: message.content,\n        onClick: onClick\n      }) : /*#__PURE__*/_jsx(StripeReturnError, {\n        title: errorMessage.title,\n        message: errorMessage.content,\n        onClick: onClick\n      })\n    })\n  });\n};","map":null,"metadata":{},"sourceType":"module"}