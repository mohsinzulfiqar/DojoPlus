{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState, useCallback } from 'react';\nimport { useTranslation } from 'react-i18next';\nimport dayjs from 'dayjs';\nimport { styled } from '@mui/material/styles';\nimport { Calendar } from '../../calendar/Calendar';\nimport { StepContainer } from '../components/StepContainer';\nimport { StepTitle } from '../components/StepTitle';\nconst TextContainer = styled('div')(({\n  theme\n}) => ({\n  paddingBottom: theme.spacing(8),\n  [theme.breakpoints.down('sm')]: {\n    paddingBottom: theme.spacing(4)\n  }\n}));\n\nconst Step3 = ({\n  data,\n  setSelected\n}) => {\n  const {\n    weekdays\n  } = data;\n  const {\n    0: current,\n    1: setCurrent\n  } = useState();\n  const onChange = useCallback(value => {\n    const selectedDay = dayjs(value.date);\n    setCurrent(selectedDay);\n    setSelected(new Date(selectedDay.toISOString()));\n  }, [setSelected]);\n  const {\n    t\n  } = useTranslation();\n  const SelectDate = t('SelectDate');\n  return /*#__PURE__*/_jsxs(StepContainer, {\n    children: [/*#__PURE__*/_jsx(TextContainer, {\n      children: /*#__PURE__*/_jsx(StepTitle, {\n        title: SelectDate,\n        subTitle: \" Timezone: Eastern Time Zone (EDT)\"\n      })\n    }), /*#__PURE__*/_jsx(Calendar, {\n      current: current,\n      dateList: weekdays.map(day => day.toLowerCase()),\n      onClick: onChange\n    })]\n  });\n};\n\nexport default Step3;","map":null,"metadata":{},"sourceType":"module"}